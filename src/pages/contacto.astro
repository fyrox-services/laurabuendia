---
import CenterContent from "@/components/CenterContent.astro";
import H1Heading from "@/components/H1Heading.astro";
import PageSection from "@/components/PageSection.astro";
import { ContactForm } from "@/components/preact/ContactForm";
import ShortDescription from "@/components/ShortDescription.astro";
import Layout from "@/layouts/Layout.astro";
import CtaLinks from "@/components/CtaLinks.astro";
---

<Layout metaTitle="Contacto" metaDescription="" ogImage="" noIndex>
  <PageSection>
    <CenterContent>
      <H1Heading modifier="contact-page"> hablémos </H1Heading>

      <ContactForm client:load heading="Rellena el formulario" />
      <ShortDescription>
        También puedes contactarme llamándome por teléfono, enviarme un WhtasApp
        o escribirme un correo electronico.
      </ShortDescription>
      <CtaLinks />
    </CenterContent>
  </PageSection>
</Layout>
